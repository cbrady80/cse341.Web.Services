{
    "env": {
        "es2021": true,
        "node" : true
    },
    "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "no-unused-vars": "warn",
        "func-names": "off"
    }
}
